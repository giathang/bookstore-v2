<h1><%= @category.category_name %></h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Isbn</th>
    <th>Des</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @category.books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.isbn %></td>
      <td><%= book.des %></td>

    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to 'Back', books_path %>